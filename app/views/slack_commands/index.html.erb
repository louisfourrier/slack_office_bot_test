

<h1 class="page-header">Listing Slack Commands Received</h1>

<div class="row">
		<div class="text-center pagination-hd">
			<%= will_paginate @slack_commands, renderer: BootstrapPagination::Rails  %>
		</div>
</div>

<table class="table">
  <thead>
    <tr>
      <th>Date</th>
      <th>Command</th>
      <th>Query</th>
      <th>Response url</th>
      <th>Understand</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @slack_commands.each do |slack_command| %>
      <tr>
        <td><%= l slack_command.created_at %></td>
        <td><%= slack_command.command %></td>
        <td><%= slack_command.query %></td>
        <td><%= slack_command.response_url %></td>
        <td><%= slack_command.understand %></td>
        <td><%= slack_command.assigned_to %></td>
        <td><%= link_to 'Voir', slack_command %></td>
        <td><%= link_to 'Edit', edit_slack_command_path(slack_command) %></td>
        <td><%= link_to 'Destroy', slack_command, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<div class="row">
		<div class="text-center pagination-hd">
			<%= will_paginate @slack_commands, renderer: BootstrapPagination::Rails  %>
		</div>
</div>

<br>
