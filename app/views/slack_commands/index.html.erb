<p id="notice"><%= notice %></p>

<h1>Listing Slack Commands</h1>

<table>
  <thead>
    <tr>
      <th>Response url</th>
      <th>Command</th>
      <th>Query</th>
      <th>Slack code</th>
      <th>Understand</th>
      <th>Assigned to</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @slack_commands.each do |slack_command| %>
      <tr>
        <td><%= slack_command.response_url %></td>
        <td><%= slack_command.command %></td>
        <td><%= slack_command.query %></td>
        <td><%= slack_command.slack_code %></td>
        <td><%= slack_command.understand %></td>
        <td><%= slack_command.assigned_to %></td>
        <td><%= link_to 'Show', slack_command %></td>
        <td><%= link_to 'Edit', edit_slack_command_path(slack_command) %></td>
        <td><%= link_to 'Destroy', slack_command, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Slack command', new_slack_command_path %>
